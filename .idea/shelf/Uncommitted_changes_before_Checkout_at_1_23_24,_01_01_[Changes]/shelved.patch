+    public ModifyBoardResponse update(Board board, ModifyBoardRequest boardDto) {
+//        Board findBoard = boardRepository.findById(id).get();
+        /*
+        Dirty Checking 발생
+         */
+        board.update(
+                boardDto.title(),
+                boardDto.content()
+        );
+
+        return ModifyBoardResponse.of(board);
